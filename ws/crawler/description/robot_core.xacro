<?xml version="1.0"?>

<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:include filename="inertial_macros.xacro" />
    <xacro:include filename="$(find ros2_livox_simulation)/urdf/mid360.xacro" />
    <xacro:property name="PI" value="3.1415926"/>


    <link name="base_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1"/>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
        </inertial>
    </link>

    <joint name="joint_chassis" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="chassis"/>
    </joint>

    <link name='base_footprint'>
    </link>

    <joint name="joint_base_footprint" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 -1.57"/>
        <parent link="base_link"/>
        <child link="base_footprint"/>
    </joint>


    <link name="chassis">
        <visual>
            <origin xyz="0 0 -0.025" rpy="0 0 0"/>
            <geometry>
                <!--
                <box size="0.4 0.4 0.05"/>
                -->
                <mesh filename="package://crawler/meshes/chassis.dae" scale="1 1 1" />
            </geometry>
            <material name="Gray">
                <color rgba="0.5 0.5 0.5 1.0"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1"/>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.4 0.4 0.05"/>
            </geometry>
        </collision>
    </link>

    <joint name="joint_front_left_wheel" type="continuous">
        <origin xyz="0.19 -0.2 0" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="front_left_wheel"/>
        <axis xyz="1 0 0"/>
        <mimic joint="joint_center_left_wheel" multiplier="1" offset="0"/>
    </joint>

    
    <joint name="joint_center_left_wheel" type="continuous">
        <parent link="chassis"/>
        <child link="center_left_wheel"/>
        <origin xyz="0.19 0 0" rpy="0 0 0"/>
        <axis xyz="1 0 0"/>
    </joint>
    
    <joint name="joint_back_left_wheel" type="continuous">
        <origin xyz="0.19 0.2 0" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="back_left_wheel"/>
        <axis xyz="1 0 0"/>
        <mimic joint="joint_center_left_wheel" multiplier="1" offset="0"/>
    </joint>

    <link name="front_left_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 ${3.141/2} 0"/>
            <geometry>
                <cylinder radius="0.0625" length="0.05"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 ${3.141/2} 0"/>
            <geometry>
                <cylinder radius="0.0625" length="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" radius="0.0625" length="0.02">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>
    <link name="center_left_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 ${3.141/2} 0"/>
            <geometry>
                <cylinder radius="0.0625" length="0.05"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 ${3.141/2} 0"/>
            <geometry>
                <cylinder radius="0.0625" length="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" radius="0.0625" length="0.02">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>

    <link name="back_left_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 ${3.141/2} 0"/>
            <geometry>
                <cylinder radius="0.0625" length="0.05"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 ${3.141/2} 0"/>
            <geometry>
                <cylinder radius="0.0625" length="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" radius="0.0625" length="0.02">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>

    <joint name="joint_front_right_wheel" type="continuous">
        <origin xyz="-0.19 -0.2 0" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="front_right_wheel"/>
        <axis xyz="1 0 0"/>
        <mimic joint="joint_center_right_wheel" multiplier="1" offset="0"/>\
    </joint>

    <joint name="joint_center_right_wheel" type="continuous">
        <origin xyz="-0.19 0 0" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="center_right_wheel"/>
        <axis xyz="1 0 0"/>
    </joint>
  
    <joint name="joint_back_right_wheel" type="continuous">
        <origin xyz="-0.19 0.2 0" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="back_right_wheel"/>
        <axis xyz="1 0 0"/>
        <mimic joint="joint_center_right_wheel" multiplier="1" offset="0"/>
    </joint>


    <link name="front_right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 ${3.141/2} 0"/>
            <geometry>
                <cylinder radius="0.0625" length="0.05"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 ${3.141/2} 0"/>
            <geometry>
                <cylinder radius="0.0625" length="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" radius="0.0625" length="0.02">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>
    <link name="center_right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 ${3.141/2} 0"/>
            <geometry>
                <cylinder radius="0.0625" length="0.05"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 ${3.141/2} 0"/>
            <geometry>
                <cylinder radius="0.0625" length="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" radius="0.0625" length="0.02">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>
    <link name="back_right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 ${3.141/2} 0"/>
            <geometry>
                <cylinder radius="0.0625" length="0.05"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 ${3.141/2} 0"/>
            <geometry>
                <cylinder radius="0.0625" length="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" radius="0.0625" length="0.02">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>
    

    <gazebo reference="back_right_wheel">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>0.1</mu>
                        <mu2>0.1</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>
        <gazebo reference="center_right_wheel">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>0.1</mu>
                        <mu2>0.1</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>
        <gazebo reference="front_right_wheel">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>0.1</mu>
                        <mu2>0.1</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>    
    <gazebo reference="back_left_wheel">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>0.1</mu>
                        <mu2>0.1</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>
        <gazebo reference="center_left_wheel">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>0.1</mu>
                        <mu2>0.1</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>
        <gazebo reference="front_left_wheel">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>0.1</mu>
                        <mu2>0.1</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>

    <xacro:mid360 name="livox" parent="base_link" topic="livox/lidar">
        <origin xyz="0 0 0.1" rpy="0 0 -1.57"/>
    </xacro:mid360>


  <joint name="imu_joint" type="fixed">
        <parent link="chassis"/>
        <child link="imu"/>
        <origin xyz="0.055 0 0.175" rpy="0 0 0"/>
    </joint>

    <link name="imu">
        <visual>
            <geometry>
                <box size="0.03 0.03 0.03"/>
            </geometry>
           
        </visual>
    </link>

  <gazebo reference="imu">
    <gravity>true</gravity>
    <sensor name="imu_sensor" type="imu">
      <always_on>true</always_on>
      <update_rate>100</update_rate>
      <visualize>true</visualize>
      <topic>default_topic</topic>
      <plugin filename="libgazebo_ros_imu_sensor.so" name="imu_plugin">
        <topicName>imu</topicName>
        <bodyName>imu</bodyName>
        <updateRateHZ>10.0</updateRateHZ>
        <gaussianNoise>0.0</gaussianNoise>
        <xyzOffset>0 0 0</xyzOffset>
        <rpyOffset>0 0 0</rpyOffset>
        <frameName>imu</frameName>
        <initial_orientation_as_reference>false</initial_orientation_as_reference>
      </plugin>
      <pose>0 0 0 0 0 0</pose>
    </sensor>
  </gazebo>

    <link name="laser_frame">
        
    </link>

    <joint name="laser_joint" type="fixed">
        <parent link="chassis"/>
        <child link="laser_frame"/>
        <origin xyz="0.1 0 0.1" rpy="0 0 0"/>
    </joint>

</robot>